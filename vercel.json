{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.json", 
      "use": "@vercel/static"
    },
    {
      "src": "**/*.xml",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.txt",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.ico",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.png",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.jpg",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.webmanifest",
      "use": "@vercel/static"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "reslinq.co.za"
        }
      ],
      "destination": "https://www.reslinq.co.za/:path*",
      "permanent": true
    }
  ]
}